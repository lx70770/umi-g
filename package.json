{
  "private": true,
  "scripts": {
    "start": "cd examples && umi dev",
    "build": "father build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@antv/g-canvas": "^0.4.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.18",
    "antd": "^4.1.1",
    "dayjs": "^1.8.23",
    "father": "^2.29.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.0.18",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/fabric": "^2.0.8",
    "eslint": "^6.8.0"
  }
}
